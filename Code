PASSWORD = input("Set Your Password.")
print ("Please enter the Selected Password")
guess = input()
if guess == PASSWORD
	print ("Correct")
	print ("Secret Info")
else:
	print("Try Again 1/3")
guess2 = input()
if guess2 == PASSWORD:
	print("Correct")
	print("Secret Info")
else:
	print("Try Again 2/3")
guess3 = input()
if guess3 == PASSWORD:
	print("Correct")
	print("Secret Info")
else:
	print("Blocked")
